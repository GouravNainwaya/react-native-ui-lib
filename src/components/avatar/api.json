{
  "name": "Avatar",
  "description": " Avatar component for displaying user profile images",
  "extends": ["TouchableOpacity", "Image"],
  "props": [
    {
      "name": "backgroundColor",
      "type": "string",
      "description": "Background color for Avatar"
    },
    {
      "name": "badgePosition",
